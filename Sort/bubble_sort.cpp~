#include <iostream>
using namespace std;
//5 1 2 4 8
//
void bubbleSort(int arr[], int n){
	for(int i=0; i < n; i++){
		bool flay=false;
		for(int j=i+1; j < n; j++){
			if(arr[j]>arr[i]){
				swap(arr[j],arr[i]);
				flay = true;
				}
			}
			if(!flay) break;
		}
	}
	
int main(){
	int arr[]={9,8,1,2,5,3,4,7};
	bubbleSort(arr,8);
	for(int i=0;i<8;i++){
		cout<<arr[i]<<" ";
		}
	
	return 0;
	}
